{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./app"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(docs)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "GenesiJS",
    "footer": "",
    "copyright": "2017 &copy; Created by blackmirror1980",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": false
  },
  "opts": {
    "template": "../ink-docstrap/template",
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "private": true
  }
}
